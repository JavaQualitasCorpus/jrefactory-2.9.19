void ExplicitConstructorInvocation() :
{
    Token tok;
    Token semicolon;
    Token dot = null;
}
{
(
  LOOKAHEAD([TypeArguments()] "this" "(" )
  [ TypeArguments() ]   // new
  tok="this" Arguments() semicolon=";" {((ASTExplicitConstructorInvocation)jjtn000).setIsThis();}
