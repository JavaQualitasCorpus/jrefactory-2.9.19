void ConstructorDeclaration() :
{
    Token tok;
    Token mod;
}
{
  [
    (mod="public"       {jjtn000.setPublic(); jjtn000.addSpecial("public", mod.specialToken);} ) |
    (mod="protected"    {jjtn000.setProtected(); jjtn000.addSpecial("protected", mod.specialToken);} ) |
