void FormalParameter() :
{
    Token tok = null;
}
{
  [ tok="final" {jjtn000.setUsingFinal(true); jjtn000.addSpecial("final", tok.specialToken); } ]
  Type() 
  [ tok="..." {jjtn000.setVarArg(true); jjtn000.addSpecial("vararg", tok.specialToken);} ]
